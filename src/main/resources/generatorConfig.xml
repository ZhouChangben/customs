<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/hgzydc?useUnicode=true&amp;characterEncoding=utf-8&amp;serverTimezone=GMT%2B8&amp;useSSL=false"
                        userId="root"
                        password="123">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!--Java类的生成器配置-->
        <javaModelGenerator targetPackage="com.xsz.customs.model" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!--Mapper的生成器-->
        <sqlMapGenerator targetPackage="mapper"  targetProject="src/main/resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.xsz.customs.mapper"  targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>
        <!--将model中的对象与数据库中的表联系起来 -->
        <table schema="DB2ADMIN" tableName="dc_user" domainObjectName="dcUser" ></table>
        <table schema="DB2ADMIN" tableName="dc_dcrw" domainObjectName="dcDcrw" ></table>
        <table schema="DB2ADMIN" tableName="dc_dwjy" domainObjectName="dcDwjy" ></table>
        <table schema="DB2ADMIN" tableName="dc_zwjy" domainObjectName="dcZwjy" ></table>
        <table schema="DB2ADMIN" tableName="dc_wsjy" domainObjectName="dcWsjy" ></table>
        <table schema="DB2ADMIN" tableName="dc_log" domainObjectName="dcLog" ></table>
        <table schema="DB2ADMIN" tableName="dj_lblx" domainObjectName="djLblx" ></table>
        <table schema="DB2ADMIN" tableName="wj_lblx" domainObjectName="wjLblx" ></table>
        <table schema="DB2ADMIN" tableName="zj_lblx" domainObjectName="zjLblx" ></table>
    </context>
</generatorConfiguration>